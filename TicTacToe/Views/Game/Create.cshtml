@{
    ViewBag.Title = "Create Game";
}

<h2>Create Game</h2>

@using (Html.BeginForm())
{
    
<div class="form-group">
        @Html.Label("Player 1")
        @Html.TextBox("player1", null, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.Label("Player 2")
        @Html.TextBox("player2", null, new { @class = "form-control" })
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
}